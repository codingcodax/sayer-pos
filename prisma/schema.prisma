generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model BookSuggestion {
  id Int @id @default(autoincrement())
  bookTitle  String
  bookAuthor String
  bookGenre String
}

model Colors {
  name String @id
  css_code String
}

model Sizes {
  name String @id
  number Int
}

model Lines {
  name String @id
}

model Categories {
  name String @id
}

model Types {
  name String @id
}
